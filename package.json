{
    "name": "caniemail-fetcher",
    "version": "1.0.0",
    "main": "dist/main.js",
    "license": "MIT",
    "type": "module",
    "files": [
        "dist"
    ],
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": "./dist/main.js",
            "require": "./dist/main.cjs"
        }
    },
    "scripts": {
        "build": "tsup",
        "ci": "bun run build && bun run check-format && bun run check-exports && bun run lint",
        "format": "prettier --write .",
        "check-format": "prettier --check . --ignore-rules=cjs-resolves-to-esm",
        "check-exports": "attw --pack .",
        "lint": "tsc",
        "snyk-test": "snyk test",
        "snyk-monitor": "snyk monitor"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "@arethetypeswrong/cli": "^0.17.1",
        "@types/bun": "^1.1.14",
        "@types/node": "^22.10.1",
        "@types/sqlite3": "^3.1.11",
        "prettier": "^3.4.2",
        "snyk": "^1.1294.2",
        "tsup": "^8.3.5",
        "typescript": "^5.7.2"
    }
}
