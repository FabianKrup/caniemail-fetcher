{
    "name": "caniemail-fetcher",
    "version": "1.0.7",
    "description": "An easy to use npm package to integrate the up to date data from the Can I emailâ€¦ project",
    "keywords": [
        "email-compatibility",
        "email-client-support",
        "can-i-email",
        "email-standards",
        "email-css-support",
        "email-html-support",
        "email-development-tools",
        "email-marketing-tools",
        "web-development",
        "email-testing",
        "email-design",
        "email-client-features",
        "cross-client-email",
        "responsive-email"
    ],
    "homepage": "https://github.com/FabianKrup/caniemail-fetcher",
    "license": "MIT",
    "type": "module",
    "files": [
        "dist"
    ],
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FabianKrup/caniemail-fetcher.git"
    },
    "scripts": {
        "build": "tsup",
        "start": "node dist/index.js",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "lint": "tsc",
        "ci": "bun run build && bun run check-format && bun run check-exports && bun run lint",
        "ci:local": "bun run build && bun run check-format && bun run check-exports && bun run lint && bun run snyk-test",
        "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
        "snyk-test": "snyk test",
        "changeset": "changeset",
        "release": "node ./scripts/release.cjs",
        "prepublishOnly": "bun run ci"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "js-yaml": "^4.1.0"
    },
    "devDependencies": {
        "@actions/exec": "^1.1.1",
        "@arethetypeswrong/cli": "^0.17.2",
        "@changesets/changelog-github": "^0.5.0",
        "@changesets/cli": "^2.27.11",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.10.2",
        "husky": "^9.1.7",
        "prettier": "^3.4.2",
        "snyk": "^1.1294.3",
        "tsup": "^8.3.5",
        "typescript": "^5.7.2"
    }
}
